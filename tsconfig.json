{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "paths": {
      "Public/*": [
        "public/*"
      ],
      "Common/*": [
        "src/common/*"
      ],
    }
  },
  "include": [
    "./src",
    "./webpack.config.ts",
    "types"
  ],
  "exclude": [
    "./.templates"
  ]
}